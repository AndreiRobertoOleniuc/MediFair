{
  "cli": {
    "version": ">= 15.0.14",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "APP_NAME": "MediFair (Dev)"
      }
    },
    "preview": {
      "distribution": "internal",
      "env": {
        "APP_NAME": "MediFair (Preview)"
      }
    },
    "production": {
      "autoIncrement": true,
      "env": {
        "APP_NAME": "MediFair"
      }
    }
  },
  "submit": {
    "production": {}
  }
}